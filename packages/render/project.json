{
	"root": "packages/render",
	"sourceRoot": "packages/render/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/render",
				"main": "packages/render/src/index.ts",
				"tsConfig": "packages/render/tsconfig.lib.json",
				"assets": ["packages/render/*.md"]
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/render/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/packages/render"],
			"options": {
				"jestConfig": "packages/render/jest.config.js",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
